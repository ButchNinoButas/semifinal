<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:background="@drawable/img_14"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <ImageView
        android:id="@+id/fishImage"
        android:layout_width="204dp"
        android:layout_height="120dp"
        android:layout_marginTop="56dp"
        app:cardCornerRadius="100dp"
        android:src="@drawable/img_7"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="ContentDescription" />

    <TextView
        android:layout_width="408dp"
        android:layout_height="738dp"
        android:text="Username:"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:textSize="20sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="285dp" />

    <EditText
        android:id="@+id/usernameEditText1"
        android:layout_width="303dp"
        android:layout_height="51dp"
        android:background="@color/white"
        android:hint=""
        android:inputType="textEmailAddress"
        android:minHeight="48dp"
        android:textColor="#000000"
        tools:ignore="MissingConstraints,TextContrastCheck,VisualLintTextFieldSize"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="316dp" />

    <TextView
        android:layout_width="408dp"
        android:layout_height="738dp"
        android:text="Password:"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="410dp" />

    <EditText
        android:id="@+id/usernameEditText2"
        android:layout_width="303dp"
        android:layout_height="51dp"
        android:background="@color/white"
        android:hint=""
        android:inputType="text"
        android:minHeight="48dp"
        android:textColor="#000000"
        tools:ignore="MissingConstraints,TextContrastCheck,VisualLintTextFieldSize"
        tools:layout_editor_absoluteX="54dp"
        tools:layout_editor_absoluteY="438dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Login"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="551dp"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>